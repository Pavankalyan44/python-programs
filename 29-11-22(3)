sno=int(input("enter student number:"))
sname=input("enter student name:")
group=input("enter student group:")
s1=int(input("enter marks:"))
s2=int(input("enter marks:"))
s2=int(input("enter marks:"))
if s1>35 and s2>35 and s2>35:
    print(sno,sname,group,"pass")
else:
   print(sno,sname,group,"fail")


